{
    "name": "nano",
    "version": "0.0.1",
    "private": true,
    "pre-commit": [],
    "lint-staged": {
      "*.{ts,tsx}": [
        "eslint --cache --fix",
        "prettier --write"
      ]
    },
    "scripts": {
      "audit": "./node_modules/.bin/resolve-audit",
      "audit:check": "./node_modules/.bin/check-audit",
      "eslint": "eslint",
      "graphql:mocks": "node ./DEV_ONLY/server.js",
      "jest": "jest src __tests__",
      "compile": "tsc",
      "lint": "eslint src --ext .js,.ts,.tsx",
      "lint:fix": "eslint src --ext .js,.ts,.tsx --fix",
      "postinstall_OLD": "npx ./node_modules/jetifier/bin/jetify && patch-package",
      "postinstall": "npx jetify && patch-package",
      "start": "npx react-native start &",
      "start:reset": "npx react-native start --reset-cache &",
      "tsc": "npx tsc",
      "restart": "npm run stop; npm run start",
      "restart:reset": "npm run stop; npm run start:reset",
      "stop": "react-native-kill-packager",
      "test": "jest --coverage --env=node --forceExit src __tests__",
      "test:watch:single": "jest --watch --env=node --forceExit",
      "test:update": "jest --watch --env=node --forceExit src __tests__ -- -u",
      "test:coverage": "jest --coverage src __tests__",
      "test:watch": "jest --watch src __tests__",
      "tslint": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.ts?(x)'",
      "version:check": "node ./scripts/check_release_version.js",
      "ios": "./config/.nano.sh && npx react-native run-ios --scheme Nano-dev --simulator \"iPhone 12\"",
      "ios-chlaffinity": "./config/.chlaffinity.sh && npx react-native run-ios --scheme Chlaffinity --simulator \"iPhone 12\"",
      "ios:sim:se": "./config/.nano.sh && npx react-native run-ios --scheme Nano-dev --simulator \"iPhone SE (2nd generation)\"",
      "ios-chlaffinity:sim:se": "./config/.chlaffinity.sh && npx react-native run-ios --scheme Chlaffinity --simulator \"iPhone SE (2nd generation)\"",
      "ios:uninstall": "xcrun simctl uninstall booted au.com.nano",
      "ios-chlaffinity:uninstall": "xcrun simctl uninstall booted au.com.chlaffinity",
      "android": "./config/.nano.sh && npx react-native run-android --variant=nanoDebug",
      "android:release": "./config/.nano.sh && npx react-native run-android --variant=nanoRelease",
      "android-chlaffinity": "./config/.chlaffinity.sh && npx react-native run-android --variant=chlaffinityDebug --appId 'au.com.chlaffinity'",
      "android-chlaffinity:release": "./config/.chlaffinity.sh && npx react-native run-android --variant=chlaffinityRelease --appId 'au.com.chlaffinity'",
      "android:bundle": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
      "android:uninstall": "adb uninstall au.com.nano",
      "android-chlaffinity:uninstall": "adb uninstall au.com.chlaffinity",
      "android-reverse": "adb reverse tcp:8081 tcp:8081",
      "install:dependencies": "npm install && npx pod-install",
      "assure:quality": "npm run compile && npm run jest",
      "graphql:codegen": "graphql-codegen --config graphql-codegen.yml",
      "graphql:codegen:watch": "graphql-codegen --config graphql-codegen.yml --watch \"src/**/*.{ts,tsx}\"",
      "deepLink:test:provision": "xcrun simctl openurl booted \"https://aucomnano.page.link/provision-apple\"",
      "deepLink:test:vaults": "xcrun simctl openurl booted \"https://aucomnano.page.link/vaults\"",
      "deepLink:test:statements": "xcrun simctl openurl booted \"https://aucomnano.page.link/statements\"",
      "deepLink:test:accounts": "xcrun simctl openurl booted \"https://aucomnano.page.link/accounts\"",
      "prepare": "husky install"
    },
